{
	"$schema": "http://json.schemastore.org/template",
	"author": "Sarin Na Wangkanai",
	"name": "Blazor Clean Architecture",
	"shortName": "blazor-clean",
	"identity": "Wangkanai.Architecture.Template",
	"sourceName": "Wangkanai.Architecture",
	"preferNameDirectory": true,
	"tags": {
		"language": "C#",
		"type": "item"
	},
	"classifications": [
		"Web",
		"Blazor",
		"Clean"
	],
	"symbols": {
	},
	"sources": [
		{
			"source": "./",
			"target": "./",
			"exclude": [
				".template.config/**/*",
				".idea/**/*",
				"templates/**/*",
				"assets/**/*",
				"artifacts/**/*",
				"packed/**/*",
				"demo/**/*",
				"**/*.filelist",
				"**/*.user",
				"**/*.lock.json",
				"**/FUNDING.yml",
				"*.nuspec",
				"*.ps1",
				"*.xml"
			],
			"rename": {
				"README-template.md": "README.md"
			}
		}
	]
}