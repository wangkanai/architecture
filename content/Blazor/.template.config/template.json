{
	"$schema": "http://json.schemastore.org/template",
	"author": "wangkanai",
	"name": "Clean Architecture Blazor Solution",
	"shortName": "clean-blazor",
	"identity": "CleanBlazor-CSharp",
	"sourceName": "Wangkanai.Architecture",
	"preferNameDirectory": true,
	"tags": {
		"language": "C#",
		"type": "project"
	},
	"classifications": [
		"Clean",
		"Web",
		"Blazor"
	],
	"symbols": {
	},
	"sources": [
		{
			"source": "./",
			"target": "./",
			"exclude": [
				".template.config/**/*",
				".idea/**/*",
				"**/*.filelist",
				"**/*.user",
				"**/*.lock.json",
				"*.nuspec"
			],
			"rename": {
				"Architecture.Blazor.sln": "Wangkanai.Architecture.sln"
			}
		}
	],
	"postActions": [
		{
			"description": "Blazor Install.ps1",
			"actionId": "210D431B-A78B-4D2F-B762-4ED3E3EA9025",
			"manualInstructions": [
				{
					"text": "Run 'Blazor install.ps1'"
				}
			],
			"args": {
				"executable": "powershell",
				"args": "-File install.ps1"
			},
			"continueOnError": true
		}
	]
}