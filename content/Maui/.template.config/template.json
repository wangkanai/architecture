{
	"$schema": "http://json.schemastore.org/template",
	"author": "wangkanai",
	"name": "Clean Architecture Maui Solution",
	"shortName": "clean-maui",
	"identity": "CleanMaui-CSharp",
	"sourceName": "Wangkanai.Architecture",
	"preferNameDirectory": true,
	"tags": {
		"language": "C#",
		"type": "project"
	},
	"classifications": [
		"Clean",
		"Maui",
		"Blazor"
	],
	"symbols": {
	},
	"sources": [
		{
			"source": "./",
			"target": "./",
			"exclude": [
				".template.config/**/*",
				".idea/**/*",
				"**/*.filelist",
				"**/*.user",
				"**/*.lock.json",
				"*.nuspec"
			],
			"rename": {
				"Architecture.Maui.sln": "Wangkanai.Architecture.sln"
			}
		}
	],
	"postActions": [
		{
			"description": "Maui Install.ps1",
			"actionId": "3d5bea9d-9f8c-4f7e-8e1b-8f4f4a7b5a4d",
			"manualInstructions": [
				{
					"text": "Run 'Maui install.ps1'"
				}
			],
			"args": {
				"executable": "powershell",
				"args": "-File install.ps1"
			},
			"continueOnError": true
		}
	]
}